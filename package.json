{
  "name": "drawio-parser-ui",
  "version": "1.0.0",
  "description": "Draw.io C4 Diagram Comparison Web UI",
  "scripts": {
    "dev": "concurrently --names \"CSS,CLJS\" --prefix-colors \"blue,green\" \"npm run css-watch\" \"shadow-cljs watch app\"",
    "css-watch": "tailwindcss -i resources/input.css -o resources/public/css/output.css --content \"./src/**/*.cljs\" --watch",
    "css-build": "tailwindcss -i resources/input.css -o resources/public/css/output.css --content \"./src/**/*.cljs\" --minify",
    "build": "npm run css-build && shadow-cljs release app",
    "build-dev": "npm run css-build && shadow-cljs compile app",
    "shadow-dev": "shadow-cljs watch app",
    "server": "shadow-cljs server",
    "repl": "shadow-cljs cljs-repl app"
  },
  "devDependencies": {
    "shadow-cljs": "^2.25.0",
    "tailwindcss": "^3.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "lucide-react": "^0.303.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3"
  }
}